{% extends 'base.html.twig' %}

{% block title %}SocialNetwork{% endblock %}

{% block body %}
<div class="container" id="down">
    <div class="row">
        <div class="col-lg-8 col-md-10 mx-auto">

        <h1 class="post-title">SocialNetwork</h1>

            <table class="table table-bordered" >
                <thead class="thead-dark">
                <tbody>
            <tr>
                <th scope="col">Id</th>
                <td>{{ social_network.id }}</td>
            </tr>
            <tr>
                <th scope="col">Name</th>
                <td>{{ social_network.name }}</td>
            </tr>
            <tr>
                <th>Client Id</th>
                <th scope="col">ClientId</th>
                <td>{{ social_network.clientId }}</td>
            </tr>
            <tr>
                <th>Client Secret</th>
                <th scope="col">ClientSecret</th>
                <td>{{ social_network.clientSecret }}</td>
            </tr>
            <tr>
                <th>Token</th>
                <td>{{ social_network.token }}</td>
            </tr>
            <tr>
                <th>Token Secret</th>
                <td>{{ social_network.tokenSecret }}</td>
            </tr>
        </tbody>
    </table>
            <div class="col-lg-10 btn-group" >
            <button type="submit" class="btn btn-outline-primary">
    <a href="{{ path('social_network_index') }}">
        <span class="btn-label"><i class="fas fa-arrow-left"></i></span>
        back to list</a></button>

            <button type="submit" class="btn btn-outline-primary">
    <a href="{{ path('social_network_edit', {'id': social_network.id}) }}"><span class="btn-label"><i class="fas fa-edit"></i></span>
        edit</a></button>

    {{ include('social_network/_delete_form.html.twig') }}
            </div>
        </div>
    </div>
</div>
{% endblock %}
